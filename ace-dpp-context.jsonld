{
  "@context": {
    "@version": 1.1,
    "dpp": "https://w3id.org/dpp/v1/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vc": "https://www.w3.org/2018/credentials#",
    "sec": "https://w3id.org/security#",

    "ace:AceDigitalProductPassport": {
      "@id": "https://sushmitha34.github.io/DPP/DPP-generic-schema.json#AceDigitalProductPassport",
      "@type": "@id"
    },

    "identification": {
      "@id": "dpp:Identification",
      "@context": {
        "uniqueProductIdentifier": "dpp:uniqueProductIdentifier",
        "dataCarrierType": "dpp:dataCarrierType",
        "productLevel": "dpp:productLevel",
        "manufacturerName": "schema:manufacturer",
        "manufacturerIdentifier": "schema:identifier",
        "manufacturingFacilityID": "dpp:manufacturingFacilityID",
        "productionDate": {
          "@id": "schema:productionDate",
          "@type": "xsd:date"
        },
        "productCategory": "schema:category",
        "batchNumber": "schema:batchNumber",
        "linkToDigitalCopy": {
          "@id": "dpp:linkToDigitalCopy",
          "@type": "@id"
        }
      }
    },

    "product": {
      "@id": "dpp:Product",
      "@context": {
        "productName": "schema:name",
        "productDescription": "schema:description",
        "brand": "schema:brand",
        "modelNumber": "schema:model",
        "classification": "dpp:classification",
        "productImage": "schema:image",
        "physicalDimension": "schema:size",
        "technicalSpecifications": "dpp:technicalSpecifications",
        "energyEfficiencyClass": "dpp:energyEfficiencyClass",
        "durabilityExpectedLifetime": "dpp:durabilityExpectedLifetime",
        "repairabilityIndex": "dpp:repairabilityIndex",
        "warrantyInformation": "schema:warranty"
      }
    },

    "sustainability": {
      "@id": "dpp:Sustainability",
      "@context": {
        "materialComposition": "dpp:materialComposition",
        "substancesOfConcern": "dpp:substancesOfConcern",
        "recycledContentPercent": "dpp:recycledContentPercent",
        "renewableContentPercent": "dpp:renewableContentPercent",
        "productEnvironmentalFootprint": "dpp:productEnvironmentalFootprint",
        "criticalRawMaterials": "dpp:criticalRawMaterials",
        "originOfMaterials": "dpp:originOfMaterials",
        "biodiversityImpact": "dpp:biodiversityImpact"
      }
    },

    "economicOperators": {
      "@id": "dpp:EconomicOperators",
      "@context": {
        "role": "schema:role",
        "name": "schema:name",
        "identifier": "schema:identifier",
        "address": "schema:address",
        "contact": "schema:contactPoint"
      }
    },

    "metadata": {
      "@id": "dpp:Metadata",
      "@context": {
        "resolverUrl": {
          "@id": "dpp:resolverUrl",
          "@type": "@id"
        },
        "registryEntryId": "dpp:registryEntryId",
        "storageLocation": "dpp:storageLocation",
        "accessRightsMetadata": "dpp:accessRightsMetadata",
        "availabilityGuarantee": "dpp:availabilityGuarantee",
        "formatAndOntology": "dpp:formatAndOntology",
        "schemaVersion": "schema:version",
        "lastUpdated": {
          "@id": "schema:dateModified",
          "@type": "xsd:dateTime"
        }
      }
    }
  }
}
